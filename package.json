{
  "name": "sql-slowlog-replay",
  "version": "1.0.0",
  "description": "Replays raw MySQL slow logs or slow logs exported from Google Cloud Logging",
  "main": "dist/index.js",
  "repository": "https://github.com/nicodoggie/sql-slowlog-replay.git",
  "author": "Nico Suarez",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "postinstall": "node scripts/npm-post-install-hook.cjs",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "check:all": "yarn lint && yarn test:verbose && yarn build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "test:coverage": "npm run test:clean && jest --coverage",
    "test:clean": "rm -rf coverage"
  },
  "devDependencies": {
    "@bmacher/eslint-config-typescript": "^1.1.0",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@tsconfig/node16": "^1.0.2",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/jsonstream": "^0.8.30",
    "@types/node": "^17.0.33",
    "@types/shelljs": "^0.8.11",
    "@types/stream-json": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "shelljs": "^0.8.5",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/ndjson": "^2.0.1",
    "commander": "^9.2.0",
    "jsonstream": "^1.0.3",
    "ndjson": "^2.0.0",
    "node-sql-parser": "^4.3.0",
    "stream-json": "^1.7.4",
    "ts-mysql-parser": "^0.2.0"
  }
}
